{
  "name": "learn-actions",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean dist && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "demo": "tsnd -r tsconfig-paths/register --transpile-only --ignore-watch --watch node_modules --respawn demo/main.ts",
    "commit": "cz",
    "test": "jest",
    "prepublish": "yarn test && yarn build"
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://github.com/souzaadriano/simple-environment-decorator",
  "dependencies": {
    "dotenv": "^16.3.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.44.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.1",
    "jest-mock-extended": "^3.0.4",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "typescript": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
